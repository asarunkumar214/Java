link  ---- https://www.geeksforgeeks.org/problems/special-series-sum0903/1



class Solution {
    static long sumOfTheSeries(long n) {
        int value = 0;
      for (int i =1; i<=n; i++){
          for(int j=1; j<=i; j++){
              value = value + j;
          }
      }return value;
        
    }
}
