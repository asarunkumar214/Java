import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in );
        int n = in.nextInt();
        int count = 0;
        for(int i=1; i<=n; i++){
            
                count = count+i ;
                System.out.println(count);
            }
        
        
    }
}      

