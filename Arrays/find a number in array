link ---- https://www.geeksforgeeks.org/problems/c-array-print-an-element-set-25933/0


code :  class Solution {
    public static int findElementAtIndex(int key, int[] arr) {
        // code here
        int a =0;
        for(int i=0; i<arr.length; i++){
            if( arr[key] == arr[i]){
                 a = a +arr[i];
                 break;
            }
        }  return a;
    }
}
